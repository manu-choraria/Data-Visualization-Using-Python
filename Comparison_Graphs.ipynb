df.plot.bar(x='Subject', y=['Very interested', 'Somewhat interested','Not interested'])

# To Plot Pie Graph
import numpy as np
labels = ['India', 'Canada', 'Japan', 'Australia', 'Russia']
size = np.array([31, 19, 15, 14, 21])
plt.pie(size, labels=labels, autopct='%.2f%%')
plt.axis('equal')
plt.show()
